// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model UserKeys {
  id  String @id @map("_id")
  secret String @unique

  createdAt DateTime @default(now())
}
